        </ParentConnector>
    </NodeDisplayFormat>

    <NodeDisplayFormat value="GroupedReference">
        <Shape value="box"/>
        <FillColor value="yellow"/>
        <Line>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="dash"/>
        </Line>
        <NameText>
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
