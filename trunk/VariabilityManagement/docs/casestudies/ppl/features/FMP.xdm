<DisplayModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="DisplayMetamodel.dmm"
 name="FMP.xdm">

    <NodeDisplayFormat value="FeatureModel">
        <Shape value="box"/>
        <FillColor value="#00C000"/>
        <Line>
            <Color value="black"/>
            <Width value="thick"/>
            <Style value="solid"/>
        </Line>
        <NameText>
            <!-- <DisplayString value="#fm:BasicProperySet/fm:FeatureNameProperty/@fm:value"/> proposal Jan 9 -->
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="large"/>
        </NameText>      
        <RootNode/>
    </NodeDisplayFormat>
    
    <NodeDisplayFormat value="RootFeature">
        <Shape value="round"/>
        <FillColor value="lightcyan"/>
        <Line>
            <Color value="black"/>
            <Width value="thick"/>
            <Style value="solid"/>
        </Line>
        <NameText>
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="large"/>
        </NameText>
        <ParentConnector>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="solid"/>
        </ParentConnector>
    </NodeDisplayFormat>

    <NodeDisplayFormat value="SolitaryFeature">
        <Shape value="box"/>
        <FillColor value="#FEEEAA"/>
        <Line>
            <Color value="black"/>
            <Width value="thick"/>
            <Style value="solid"/>
        </Line>
        <NameText>
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="large"/>
        </NameText>
        <CardinalityText>
            <VerticalPosition value="top-outside"/>
            <HorizontalPosition value="right-outside"/>
            <Color value="red"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="normal"/>
            <Line>
                <Color value="black"/>
                <Width value="normal"/>
                <Style value="solid"/>
            </Line>
            <FillColor value="yellow"/>
        </CardinalityText>
        <ParentConnector>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="solid"/>
        </ParentConnector>
    </NodeDisplayFormat>

    <NodeDisplayFormat value="SolitaryReference">
