            <FillColor value="yellow"/>
        </CardinalityText>
        <ParentConnector>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="solid"/>
        </ParentConnector>
    </NodeDisplayFormat>

    <NodeDisplayFormat value="GroupedFeature">
        <Shape value="box"/>
        <FillColor value="lightyellow"/>
        <Line>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="dash"/>
        </Line>
        <NameText>
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
            <FontStyle value="bold"/>
