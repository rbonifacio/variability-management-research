            <FontSize value="large"/>
        </NameText>
        <ParentConnector>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="solid"/>
        </ParentConnector>
    </NodeDisplayFormat>
    
    <NodeDisplayFormat value="FeatureGroup">
       <Shape value="dot"/>
       <FillColor value="lightgreen"/><!-- O.R. It would be fine to have it changeable according to the group cardinality. Is it worth doing? -->
       <CardinalityText>
            <VerticalPosition value="bottom-outside"/>
            <HorizontalPosition value="right-outside"/>
            <Color value="green"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="normal"/>
            <Line>
                <Color value="black"/>
                <Width value="thick"/>
                <Style value="solid"/>
            </Line>
            <FillColor value="transparent"/>
        </CardinalityText>
        <AttachedToParent/>
    </NodeDisplayFormat>
      
    <NodeDisplayFormat value="Attribute">
        <Shape value="dot"/>
        <Size value="20"/>
        <FillColor value="red"/><!-- O.R. It would be fine to have it changeable according to the group cardinality. Is it worth doing? -->
        <AttachedToParent>
            <VerticalPosition value="top-outside"/>
            <HorizontalPosition value="center"/>
        </AttachedToParent>
    </NodeDisplayFormat>

    <NodeDisplayFormat value="Integer">
        <Shape value="dot"/>
        <Size value="12"/>
        <FillColor value="blue"/><!-- O.R. It would be fine to have it changeable according to the group cardinality. Is it worth doing? -->
        <AttachedToParent>
            <VerticalPosition value="top-inside"/>
            <HorizontalPosition value="left-inside"/>
        </AttachedToParent>
    </NodeDisplayFormat>
    <NodeDisplayFormat value="String">
        <Shape value="dot"/>
        <Size value="12"/>
        <FillColor value="cadetblue"/><!-- O.R. It would be fine to have it changeable according to the group cardinality. Is it worth doing? -->
        <AttachedToParent>
            <VerticalPosition value="top-inside"/>
            <HorizontalPosition value="left-inside"/>
        </AttachedToParent>
    </NodeDisplayFormat>
    <NodeDisplayFormat value="Float">
        <Shape value="dot"/>
