<configurationModel name="Configuration">
 <configuration expression="eShop">
   <transformation name="selectScenarios" args="SC01,SC02"/>
 </configuration>
 <configuration expression="ShippingMethod">
    <transformation name="bindParameter" args="SM,ShippingMethod"/>
 </configuration>
  <configuration expression="Not (And (ShoppingCart, Bonus))">
    <transformation name="evaluateAspect" args="ADV01"/>
  </configuration> 
  <configuration expression="And (ShoppingCart, Bonus)">
    <transformation name="evaluateAspect" args="ADV02"/>
  </configuration>
  <configuration expression="UpdatePreferences">
    <transformation name="evaluateAspect" args="ADV03"/>
  </configuration>
</configurationModel>